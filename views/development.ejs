<!DOCTYPE html>
<html lang="en" ng-app="firstapp">

<head ng-controller="headerctrl">
    <meta charset="utf-8" />
    <base href="/" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta name="description" content="" />
    <meta name="author" content="" />
    <meta name="robots" content="noindex" />
    <meta name="googlebot" content="noindex" />
    <meta name="theme-color" content="#f37021">
    <link rel="shortcut icon" href="frontend/img/favicon.png" />

    <title ng-bind="'Dharma Productions | '+template.title">
        <%= ((typeof title) == "undefined") ? "Dharma Productions" : title  %>
    </title>

    <!-- SEO -->
    <meta name="author" content="Dharma Productions" />
    <meta name="description" content="<%= ((typeof description) == "undefined") ? "Dharma Productions " : description  %>" />
    <meta name="keywords" content="<%= ((typeof keywords) == "undefined") ? "Dharma Productions " : keywords  %>" />

    <!-- Facebook -->
    <meta property="og:site_name" content="Dharma Productions">
    <meta property="og:title" content="<%= ((typeof title) == "undefined") ? "Dharma Productions " : title  %>" />
    <meta property="og:description" content="Dharma Productions is a leading Indian Film Production and Distribution company. Owned by Karan Johar, the company was founded by his father Late Shri Yash Johar in 1976." />
    <meta property="og:image"  content="https://storage.googleapis.com/twit-banner/tw-banner.jpg"/>
    <meta property="og:type" content="website" />
    <meta property="og:url" content="<%= ((typeof url) == "undefined") ? "Dharma Productions" : url  %>" />


    <!-- Twitter -->
    <meta name="twitter:card" content="summary_large_image" />
  <meta name="twitter:site" content="@DharmaMovies" />
  <meta name="twitter:title" content="<%= ((typeof title) == "undefined") ? "Dharma Productions " : title  %>" />
  <meta name="twitter:image" content="https://storage.googleapis.com/twit-banner/tw-banner.jpg" />
  <meta name="twitter:image:alt" content="<%= ((typeof description) == "undefined") ? "Dharma Productions " : description  %>" />
  <meta name="twitter:description" content="<%= ((typeof description) == "undefined") ? "Dharma Productions " : description  %>" />


<link rel="canonical" href="<%= ((typeof url) == "undefined") ? "Dharma Productions " : url  %>">



    <!--[if IE]> <script src="https://cdn.jsdelivr.net/html5shiv/3.7.2/html5shiv.min.js"></script> <script src="https://cdn.jsdelivr.net/respond/1.4.2/respond.min.js"></script> <![endif]-->
    <!--[if IE]> <script src="https://cdn.jsdelivr.net/html5shiv/3.7.2/html5shiv.min.js"></script> <script src="https://cdn.jsdelivr.net/respond/1.4.2/respond.min.js"></script> <![endif]-->
    <script language=JavaScript>
    var message="Function Disabled!";
    function clickIE4(){
    if (event.button==2){
    return false;
    }
    }
    function clickNS4(e){
    if (document.layers||document.getElementById&&!document.all){
    if (e.which==2||e.which==3){
    return false;
    }
    }
    }
    if (document.layers){
    document.captureEvents(Event.MOUSEDOWN);
    document.onmousedown=clickNS4;
    }
    else if (document.all&&!document.getElementById){
    document.onmousedown=clickIE4;
    }
    document.oncontextmenu=new Function("return false")
    </script>
</head>

<body>
    <div class="repeated-item" ui-view></div>


    <link rel="stylesheet" type="text/css" href="frontend/css/main.css" />

    <script>
        var adminurl = "<%= ((typeof adminurl) == "undefined") ? "" : adminurl  %>";
        var isproduction = true;
    </script>
